//GFG -- Easy -- Expedia
//C++ Code
#include <bits/stdc++.h>
using namespace std;
class Solution {
  public:
    long long int minOperations(int N) {
        // Code here
        return (long long)N*N/4;
    }
};
int main() {
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        Solution ob;
        cout << ob.minOperations(n) << endl;
    }
    return 0;
}
